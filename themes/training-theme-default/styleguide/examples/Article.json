{
  "_include": "/core/page/Page.json",
  "_hidden": false,
  "_template": "/core/article/ArticlePage.hbs",
  "dateExpired": "December 11, 2018",
  "dateExpiredISO": "2018-02-19T17:44:23+00:00",
  "datePublished": "December 11, 2016",
  "datePublishedISO": "2016-02-19T16:43:22+00:00",
  "dateModified": "December 12, 2016",
  "dateModifiedISO": "2016-02-19T17:44:23+00:00",
  "headline": "{{words}}",
  "subHeadline": "{{words}}",
  "authorUrl": "#",
  "authorImage": {
    "_image": true
  },
  "banner": {
    "_include": "/core/banner/Banner.json"
  },
  "authorName": "{{name}}",
  "authorBiography": "{{sentences}}",
  "contributors": [
    {
      "_include": "/core//link/Link.json",
      "_repeat": 2,
      "body": "{{name}}"
    }
  ],
  "above": [
    {
      "_include": "/dfp/GoogleDfpAdModule.json"
    }
  ],

  "aside": [
    {
      "_include": "/core/list/ListB.json"
    }
  ],

  "breadcrumbs": [
    {
        "_include": "/core/link/Link.json",
        "_repeat": [ 2, 3 ]
    }
  ],

  "lead": {
    "_include": "/core/figure/Figure.json"
  },

  "articleBody": {
    "_template": "/core/article/RichTextArticleBody.hbs",
    "body": [
      "{{paragraphs(2)}}",
      "{{html('<div class=\"Corrections\" data-with-aside=\"true\"><div class=\"Revision-header\">Corrections:</div><div class=\"Revision\"><span class=\"Revision-date\">8:10 p.m:</span><span class=\"Revision-message\">Important correction should go here with more text.</span></div></div>')}}",
      "{{paragraphs([ 2, 3 ])}}",
      {
        "_template": "/core/enhancement/Enhancement.hbs",

        "item": {
          "_include": "/core/figure/Figure.json"
        },
        "inline": false
      },
      "{{paragraphs([ 2, 3 ])}}",
      {
        "_template": "/core/enhancement/Enhancement.hbs",

        "item": {
          "_include": "/core/carousel/Carousel.json"
        },
        "inline": false
      },
      "{{paragraphs([ 2, 3 ])}}",
      {
        "_template": "/core/enhancement/Enhancement.hbs",

        "enhancementAlignment": "align-left",
        "item": {
          "_include": "/core/quote/Quote.json"
        },
        "inline": false
      },
      "{{paragraphs([ 2, 3 ])}}"
    ]
  },

  "tags": [
    {
      "_include": "/core/link/Link.json",
      "_repeat": 4
    }
  ]
}
