<!DOCTYPE html>
<html class="BlogPage">
{{include "/page/Page-head.hbs" this}}
<body class="Page-body">
    {{include "/page/Page-bodyStart.hbs" this}}
    {{include "/page/Page-header.hbs" this}}

    <div data-moduleroot>
        {{include "/page/Page-above.hbs" this}}

        <main class="Page-content">
            {{#with pageLead}}
                <div class="Page-pageLead" data-pagelead>{{this}}</div>
            {{/with}}

            {{#if sectionNavigation}}
                {{include "/section/SectionNavigation.hbs" this}}
            {{/if}}

            {{#if aside}}
                    <div class="Page-twoColumn">
                        <div class="Page-main" data-modulewell>
                        {{#each main}}
                            {{render this [/page/list/PageList.hbs]="/page/list/PageListStandardD.hbs"}}
                        {{/each}}
                        </div>
                        <aside class="Page-aside">
                            {{aside}}
                        </aside>
                    </div>
            {{else}}
                {{#with main}}
                    <main class="Page-oneColumn">
                        {{#each this}}
                            {{render this [/page/list/PageList.hbs]="/page/list/PageListStandardD.hbs"}}
                        {{/each}}
                    </main>
                {{/with}}
            {{/if}}
        </main>

        {{include "/page/Page-below.hbs" this dataClick="below"}}
    </div>
    {{include "/page/Page-footer.hbs" this}}
    {{include "/page/Page-extraBodyItems.hbs" this}}
</body>
</html>
