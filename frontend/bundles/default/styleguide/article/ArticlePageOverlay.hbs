<!DOCTYPE html>
<html class="ArticlePage" {{~include "/article/includes/ArticlePageAttributes.hbs" this leadStyle="overlay"}}>
    {{include "/page/Page-head.hbs" this}}

    <body class="Page-body">
        {{include "/page/Page-bodyStart.hbs" this}}
        {{include "/page/Page-header.hbs" this}}
        <div data-moduleroot>
            {{include "/page/Page-above.hbs" this}}
            {{include "/page/Page-sponsoredBanner.hbs" this}}

            <main class="Page-content">
                {{include '/article/includes/ArticlePageOverlayLead.hbs' this}}

                {{#if aside}}<div class="Page-twoColumn">{{else}}<div class="Page-oneColumn" data-modulewell>{{/if}}
                    <div class="Page-main">
                        {{#if (or datePublished readTime authors actions sponsored)}}
                            {{include '/global/AssetTitle.hbs' this assetTitleTextAlign="left" breadcrumbs=null headline=null subHeadline=null}}
                        {{/if}}
                        {{include "/article/includes/ArticlePageBody.hbs" this}}
                    </div>

                    {{include '/page/Page-aside.hbs' this}}
                </div>
            </main>

            {{include "/page/Page-below.hbs" this}}
        </div>
        {{include "/page/Page-footer.hbs" this}}
        {{include "/page/Page-extraBodyItems.hbs" this}}
    </body>
</html>
