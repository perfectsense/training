{{#with title}}<itunes:title>{{this}}</itunes:title>{{/with}}
{{#with summary}}<itunes:summary><![CDATA[{{this}}]]></itunes:summary>{{/with}}
{{#with subtitle}}<itunes:subtitle><![CDATA[{{this}}]]></itunes:subtitle>{{/with}}
{{#with author}}<itunes:author>{{this}}</itunes:author>{{/with}}
{{#with duration}}<itunes:duration>{{this}}</itunes:duration>{{/with}}
{{#with imageUrl}}<itunes:image href="{{this}}" />{{/with}}
{{#with explicit}}<itunes:explicit>{{this}}</itunes:explicit>{{/with}}
{{#with episodeType}}<itunes:episodeType>{{this}}</itunes:episodeType>{{/with}}
{{#with episodeNumber}}<itunes:episode>{{this}}</itunes:episode>{{/with}}
{{#with seasonNumber}}<itunes:season>{{this}}</itunes:season>{{/with}}
{{#with block}}<itunes:block>{{this}}</itunes:block>{{/with}}