{{#if items}}
<bsp-listcarousel class="PageListCarouselC" data-module data-has-background {{include "includes/PageList-attributes.hbs" this}}>
    {{#with items}}
        <div class="PageList-items">
            {{#each this}}
                <div class="PageList-items-item">
                    {{#set lazyLoad="flickity"}}
                        {{#eq this._template "/page/promo/PagePromo.hbs"}}
                            {{include "/page/promo/PagePromoModuleB.hbs" this
                                useTextColors=../../useTextColors
                            }}
                        {{else}}
                            {{render this}}
                        {{/eq}}
                    {{/set}}
                </div>
            {{/each}}
        </div>
    {{/with}}
</bsp-listcarousel>
{{/if}}
