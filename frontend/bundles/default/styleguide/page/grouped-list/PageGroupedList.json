{
  "_template": "./PageGroupedList.hbs",
  "title": "{{words([4,8])}}",
  "description": "{{words([10,20])}}",
  "anchorId": "list-anchor",
  "groups": [
    {
      "_include": "/page/list/PageList.json",
      "_repeat": 6,
      "items": [
        {
          "_include": "/page/promo/PagePromo.json",
          "_repeat": 3
        }
      ]
    }
  ],
  "ctaButtonText": "{{words([1,2])}}",
  "ctaUrl": "#",
  "ctaTarget": "_self",
  "pagination": {
    "_include": "/link/PaginationLink.json"
  }
}
