{{#if items}}
<div class="PageListY" data-module {{include "includes/PageList-attributes.hbs" this}}>
    {{include "includes/PageList-header.hbs" this}}

    {{#with items}}
        <div class="PageListY-items">
            {{#each this}}
                <div class="PageListY-items-item">
                    {{#eq this._template "/page/promo/PagePromo.hbs"}}
                        {{include "/page/promo/PagePromoC.hbs" this}}
                    {{else}}
                        {{render this}}
                    {{/eq}}
                </div>
            {{/each}}
        </div>
    {{/with}}

    {{include "includes/PageList-footer.hbs" this}}
</div>
{{/if}}
